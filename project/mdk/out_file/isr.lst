  213            * @brief TIM0中断处理函数，提取imu数据
  214            * @param 无
  215            * @return 无
  216            */
  217          void pit_hanlder_imu(void)
  218          {
  219   1          int i=0; 
  220   1        
  221   1          imu660ra_get_acc();  // 获取 IMU660RA 的加速度测量数值
  222   1          imu660ra_get_gyro(); // 获取 IMU660RA 的角速度测量数值
  223   1          
  224   1          //转换为实际物理值
  225   1          
  226   1          imu_acc[0]=imu660ra_acc_transition(imu660ra_acc_x)-0.025;
  227   1          imu_acc[1]=imu660ra_acc_transition(imu660ra_acc_y)+0.008;
  228   1          imu_acc[2]=imu660ra_acc_transition(imu660ra_acc_z)+0.005;
  229   1        
  230   1          for(i=0;i<2;i++)
  231   1            if(abs(imu_acc[i])<0.01)imu_acc[i]=0;
  232   1          if(abs(imu_acc[2])-1<0.01)imu_acc[2]=1;
  233   1       
  234   1          imu_gyro[0]=imu660ra_gyro_transition(imu660ra_gyro_x);
  235   1          imu_gyro[1]=imu660ra_gyro_transition(imu660ra_gyro_y);
  236   1          imu_gyro[2]=imu660ra_gyro_transition(imu660ra_gyro_z);
  237   1        
  238   1          for(i=0;i<3;i++)
  239   1            if(abs(imu_gyro[i])<0.15)imu_gyro[i]=0;
  240   1        
  241   1          //互补滤波
  242   1          complementary_filter(imu_acc,imu_gyro,0.005);
  243   1          
  244   1      }
